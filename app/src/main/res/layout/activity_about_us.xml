<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:orientation="vertical"
    android:layout_height="match_parent">
<include layout="@layout/top_bar"/>
    <ImageView
        android:id="@+id/iv_icon"
        android:background="@mipmap/x_logo"
        android:layout_marginTop="120dp"
        android:layout_centerHorizontal="true"
        android:layout_width="100dp"
        android:layout_height="100dp" />
    <TextView
        android:gravity="center"
        android:textColor="@color/colorTextBlack"
        android:id="@+id/tv_v"
        android:text="V 1.0.0"
        android:textSize="15sp"
        android:layout_marginTop="15dp"
        android:layout_below="@+id/iv_icon"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />
    <View
        android:id="@+id/v_1"
        android:layout_marginTop="30dp"
        android:layout_below="@+id/tv_v"
        android:background="@color/themeGrayLine"
        android:layout_width="match_parent"
        android:layout_height="0.5dp"/>
    <RelativeLayout
        android:layout_below="@+id/tv_v"
        android:id="@+id/layout_call_sevice"
        android:paddingRight="15dp"
        android:paddingLeft="15dp"
        android:background="@color/themeWhite"
        android:layout_width="match_parent"
        android:layout_height="40dp">
        <TextView
            android:textSize="12sp"
            android:textColor="@color/colorTextBlack"
            android:layout_centerVertical="true"
            android:text="联系客服"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />
        <TextView
            android:textSize="12sp"
            android:layout_alignParentRight="true"
            android:textColor="@color/themeGrayText"
            android:layout_centerVertical="true"
            android:text=">"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />
    </RelativeLayout>
    <View
        android:id="@+id/v_2"
        android:layout_below="@+id/layout_call_sevice"
        android:background="@color/themeGrayLine"
        android:layout_width="match_parent"
        android:layout_height="0.5dp"/>
    <RelativeLayout
        android:layout_below="@+id/v_2"
        android:id="@+id/layout_feed_back"
        android:paddingRight="15dp"
        android:paddingLeft="15dp"
        android:background="@color/themeWhite"
        android:layout_width="match_parent"
        android:layout_height="40dp">
        <TextView
            android:textSize="12sp"
            android:textColor="@color/colorTextBlack"
            android:layout_centerVertical="true"
            android:text="意见反馈"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />
        <TextView
            android:textSize="12sp"
            android:layout_alignParentRight="true"
            android:textColor="@color/themeGrayText"
            android:layout_centerVertical="true"
            android:text=">"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />
    </RelativeLayout>
    <View
        android:id="@+id/v_3"
        android:layout_below="@+id/layout_feed_back"
        android:background="@color/themeGrayLine"
        android:layout_width="match_parent"
        android:layout_height="0.5dp"/>
    <RelativeLayout
        android:layout_below="@+id/v_3"
        android:id="@+id/layout_clear"
        android:paddingRight="15dp"
        android:paddingLeft="15dp"
        android:background="@color/themeWhite"
        android:layout_width="match_parent"
        android:layout_height="40dp">
        <TextView
            android:textSize="12sp"
            android:textColor="@color/colorTextBlack"
            android:layout_centerVertical="true"
            android:text="清理缓存"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />
        <TextView
            android:textSize="12sp"
            android:layout_alignParentRight="true"
            android:textColor="@color/themeGrayText"
            android:layout_centerVertical="true"
            android:text=">"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />
    </RelativeLayout>
    <LinearLayout
        android:gravity="center"
        android:orientation="vertical"
        android:layout_marginBottom="40dp"
        android:layout_alignParentBottom="true"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" >

        <TextView
            android:id="@+id/tv_copy"
            android:text="Copyright©2019"
            android:textSize="13sp"
            android:textColor="@color/themeGrayText"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />
        <TextView
            android:text="杭州傲宇数码科技有限公司"
            android:textSize="13sp"  android:layout_marginTop="5dp"
            android:textColor="@color/themeGrayText"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />
        <TextView
            android:id="@+id/tv_net"
            android:layout_marginTop="5dp"
            android:textSize="13sp"
            android:textColor="@color/themeGrayText"
            android:text=" All Rights Reserved www.aoskytech.net"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />

    </LinearLayout>
</RelativeLayout>
